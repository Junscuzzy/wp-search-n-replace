{
  "extends": ["standard", "prettier", "prettier/react", "plugin:react/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "import/prefer-default-export": 0,
    "prettier/prettier": 2,
    "react/prop-types": 0
  },
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
    "FormData": true,
    "FileReader": true,
    "Blob": true,
    "navigator": true,
    "Headers": true,
    "Request": true,
    "fetch": true
  },
  "parser": "babel-eslint"
}